{
   "version": 2,
   "builds": [
     {
       "src": "api/app.js",
       "use": "@vercel/node"
     },
     {
       "src": "public/**/*",
       "use": "@vercel/static"
     }
   ],
   "routes": [
     {
       "src": "/code-amar/v1/(.*)",
       "dest": "api/app.js"
     },
     {
       "src": "/public/(.*)",
       "dest": "/public/$1"
     }
   ]
 }
 